<% include ../shared/header %>
	<div class="ts-container">
		<section class="ts-text-container">

		<div class="ts-row">
			<div class="ts-col-span8 ts-col-nofloat">
				<h2 class="text-left"><%= pagedata.title %></h2>
				<div class="ts-padding-lg">
					<% if(locals.isLoggedIn()){
						%>
							<div class="ts-text-center">
								Already Logged In, <a href="/auth/logout">Log Out</a>
							</div>
						<%
					} 
					else{
						%>
							<form action="/auth/user/new" method="post" autocomplete="on" id="user_form" name="user_form" class="ts-form ">
								<% include register_form %>
							</form>
						<%
					} %>
				</div>
			</div>
		</div>
		</section>
	</div>
<% include ../shared/footer %>